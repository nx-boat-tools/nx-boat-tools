{
  "$schema": "./node_modules/@angular-devkit/architect/src/builders-schema.json",
  "builders": {
    "build-dotnet": {
      "implementation": "./builders/dotnetBuilder/dotnetBuilder",
      "schema": "./builders/dotnetBuilder/schema.json",
      "description": "Build a .Net Core project."
    },
    "package-dotnet": {
      "implementation": "./builders/dotnetPackager/dotnetPackager",
      "schema": "./builders/dotnetPackager/schema.json",
      "description": "Package a .Net Core project with nuget."
    },
    "run-dotnet": {
      "implementation": "./builders/dotnetRunner/dotnetRunner",
      "schema": "./builders/dotnetRunner/schema.json",
      "description": "Run a .Net Core project."
    },
    "clean-dotnet": {
      "implementation": "./builders/dotnetCleaner/dotnetCleaner",
      "schema": "./builders/dotnetCleaner/schema.json",
      "description": "Clean a .Net Core project."
    }
  }
}
