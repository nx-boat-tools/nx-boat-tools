{
    "$schema": "./node_modules/@angular-devkit/schematics/collection-schema.json",
    "schematics": {
      "api": {
        "description": "Generate a .Net Core API project.",
        "factory": "./schematics/dotnet-api/index",
        "schema": "./schematics/dotnet-api/schema.json"
      },
      "console-app": {
        "description": "Generate a .Net Core console project.",
        "factory": "./schematics/dotnet-console/index",
        "schema": "./schematics/dotnet-console/schema.json"
      },
      "classlib": {
        "description": "Generate a .Net Core class library.",
        "factory": "./schematics/dotnet-library/index",
        "schema": "./schematics/dotnet-library/schema.json"
      }
    }
  }
