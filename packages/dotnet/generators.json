{
  "$schema": "http://json-schema.org/schema",
  "name": "dotnet",
  "version": "0.0.1",
  "generators": {
    "project": {
      "factory": "./src/generators/project/generator",
      "schema": "./src/generators/project/schema.json",
      "description": "dotnet project generator"
    },
    "classlib": {
      "factory": "./src/generators/classlib/generator",
      "schema": "./src/generators/classlib/schema.json",
      "description": "dotnet class library generator"
    },
    "console": {
      "factory": "./src/generators/console/generator",
      "schema": "./src/generators/console/schema.json",
      "description": "dotnet console app generator"
    },
    "grpc": {
      "factory": "./src/generators/grpc/generator",
      "schema": "./src/generators/grpc/schema.json",
      "description": "dotnet grpc app generator"
    },
    "test": {
      "factory": "./src/generators/test/generator",
      "schema": "./src/generators/test/schema.json",
      "description": "dotnet test generator"
    },
    "project-ref": {
      "factory": "./src/generators/project-ref/generator",
      "schema": "./src/generators/project-ref/schema.json",
      "description": "dotnet project-ref generator"
    },
    "webapi": {
      "factory": "./src/generators/webapi/generator",
      "schema": "./src/generators/webapi/schema.json",
      "description": "dotnet web API generator"
    }
  }
}
