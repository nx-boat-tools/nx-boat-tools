{
  "version": 2,
  "projects": {
    "common": {
      "root": "packages/common",
      "sourceRoot": "packages/common/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/common/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/common"],
          "options": {
            "jestConfig": "packages/common/jest.config.js",
            "passWithNoTests": true,
            "coverage": true,
            "coverageProvider": ["json", "html"]
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/common",
            "tsConfig": "packages/common/tsconfig.lib.json",
            "packageJson": "packages/common/package.json",
            "main": "packages/common/src/index.ts",
            "assets": [
              "packages/common/*.md",
              {
                "input": "./packages/common/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./packages/common/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./packages/common",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/common",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "common-e2e": {
      "root": "e2e/common-e2e",
      "projectType": "application",
      "sourceRoot": "e2e/common-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "common:build",
            "jestConfig": "e2e/common-e2e/jest.config.js"
          }
        }
      }
    },
    "docker": {
      "root": "packages/docker",
      "sourceRoot": "packages/docker/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/docker/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/docker"],
          "options": {
            "jestConfig": "packages/docker/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/docker",
            "tsConfig": "packages/docker/tsconfig.lib.json",
            "packageJson": "packages/docker/package.json",
            "main": "packages/docker/src/index.ts",
            "assets": [
              "packages/docker/*.md",
              {
                "input": "./packages/docker/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./packages/docker/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./packages/docker",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/docker",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "docker-e2e": {
      "root": "e2e/docker-e2e",
      "projectType": "application",
      "sourceRoot": "e2e/docker-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "docker:build",
            "jestConfig": "e2e/docker-e2e/jest.config.js"
          }
        }
      }
    },
    "dotnet": {
      "root": "packages/dotnet",
      "sourceRoot": "packages/dotnet/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/dotnet/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/dotnet"],
          "options": {
            "jestConfig": "packages/dotnet/jest.config.js",
            "passWithNoTests": true,
            "coverage": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/dotnet",
            "tsConfig": "packages/dotnet/tsconfig.lib.json",
            "packageJson": "packages/dotnet/package.json",
            "main": "packages/dotnet/src/index.ts",
            "assets": [
              "packages/dotnet/*.md",
              {
                "input": "./packages/dotnet/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./packages/dotnet/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./packages/dotnet",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/dotnet",
                "glob": "executors.json",
                "output": "."
              },
              {
                "input": "./packages/dotnet/src/generators",
                "glob": "**/files/**/*",
                "output": "./templates"
              }
            ]
          }
        }
      }
    },
    "dotnet-e2e": {
      "root": "e2e/dotnet-e2e",
      "projectType": "application",
      "sourceRoot": "e2e/dotnet-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "dotnet:build",
            "jestConfig": "e2e/dotnet-e2e/jest.config.js"
          }
        }
      }
    },
    "helm": {
      "root": "packages/helm",
      "sourceRoot": "packages/helm/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/helm/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/helm"],
          "options": {
            "jestConfig": "packages/helm/jest.config.js",
            "passWithNoTests": true,
            "coverage": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/helm",
            "tsConfig": "packages/helm/tsconfig.lib.json",
            "packageJson": "packages/helm/package.json",
            "main": "packages/helm/src/index.ts",
            "assets": [
              "packages/helm/*.md",
              {
                "input": "./packages/helm/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./packages/helm/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./packages/helm",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/helm",
                "glob": "executors.json",
                "output": "."
              },
              {
                "input": "./packages/helm/src/generators",
                "glob": "**/files/**/*",
                "output": "./templates"
              }
            ]
          }
        }
      }
    },
    "helm-e2e": {
      "root": "e2e/helm-e2e",
      "projectType": "application",
      "sourceRoot": "e2e/helm-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "helm:build",
            "jestConfig": "e2e/helm-e2e/jest.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
