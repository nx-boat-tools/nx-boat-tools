{
  "npmScope": "src",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "common": {
      "tags": []
    },
    "common-e2e": {
      "tags": [],
      "implicitDependencies": ["common"]
    },
    "docker": {
      "tags": []
    },
    "docker-e2e": {
      "tags": [],
      "implicitDependencies": ["docker"]
    },
    "dotnet": {
      "tags": [],
      "implicitDependencies": ["common"]
    },
    "dotnet-e2e": {
      "tags": [],
      "implicitDependencies": ["common", "dotnet"]
    },
    "helm": {
      "tags": []
    },
    "helm-e2e": {
      "tags": [],
      "implicitDependencies": ["helm"]
    }
  },
  "workspaceLayout": {
    "appsDir": "e2e",
    "libsDir": "packages"
  }
}
